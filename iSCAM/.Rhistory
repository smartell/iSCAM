text(median(fe),ye[imsy],paste("Umsy =", fe[imsy],"\nMSY =", round(ye[imsy], 0)), pos=1)#
segments(fe[imsy],0, fe[imsy], ye[imsy],col="grey",lty=2)#
segments(0,ye[imsy], fe[imsy], ye[imsy],col="grey",lty=2)#
